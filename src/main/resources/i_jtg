CREATE TABLE `i_jtg_evacuate_plan` (
  `i_jtg_ep_id` int(11) NOT NULL AUTO_INCREMENT COMMENT '临时表ID',
  `port_flag` varchar(50) NOT NULL COMMENT '港口',
  `steel_flag` varchar(50) NOT NULL COMMENT '钢厂',
  `plan_no` varchar(50) NOT NULL COMMENT '计划号',
  `plan_flag` varchar(50) NOT NULL COMMENT '计划ID',
  `time_index` varchar(50) DEFAULT NULL COMMENT '索引时间',
  `wharf_code` varchar(50) NOT NULL COMMENT '作业码头code',
  `wharf_name` varchar(50) DEFAULT NULL COMMENT '作业码头',
  `area_name` varchar(50) DEFAULT NULL COMMENT '库区code',
  `area_address` varchar(50) DEFAULT NULL COMMENT '货位',
  `fleet_code` varchar(50) NOT NULL COMMENT '作业车队code',
  `fleet_name` varchar(50) NOT NULL COMMENT '作业车队',
  `avalid_date` varchar(50) DEFAULT NULL,
  `worktype` varchar(50) DEFAULT NULL,
  `receiver_name` varchar(50) DEFAULT NULL COMMENT '收货钢厂(钢厂流向)',
  `direction_name` varchar(50) DEFAULT NULL,
  `check_liuxiang` varchar(50) DEFAULT NULL,
  `ship_name` varchar(50) DEFAULT NULL COMMENT '船名',
  `voyage_number` varchar(50) DEFAULT NULL COMMENT '航次/船号',
  `max_vehiclenum` varchar(50) DEFAULT NULL,
  `forwarder_name` varchar(50) DEFAULT NULL COMMENT '货代',
  `goods_name` varchar(50) NOT NULL COMMENT '货名',
  `plan_weight` varchar(50) DEFAULT NULL COMMENT '计划吨数',
  `plan_pieces` varchar(50) DEFAULT NULL COMMENT '计划件数',
  `plan_out_weight` varchar(50) DEFAULT NULL,
  `control_weight` varchar(50) DEFAULT NULL COMMENT '货主委派车队发的计划量',
  `start_time` varchar(50) DEFAULT NULL COMMENT '开始时间',
  `end_time` varchar(50) DEFAULT NULL COMMENT '结束时间',
  `end_date` varchar(50) NOT NULL COMMENT '办单有效期',
  `corp_name` varchar(50) DEFAULT NULL,
  `remark` varchar(200) DEFAULT NULL COMMENT '备注',
  `record_user` varchar(50) DEFAULT NULL COMMENT '记录人',
  `record_date` varchar(50) DEFAULT NULL COMMENT '记录时间',
  `update_user` varchar(50) DEFAULT NULL COMMENT '修改人',
  `update_date` varchar(50) DEFAULT NULL COMMENT '修改时间',
  `if_check_result` char(1) NOT NULL COMMENT '接口数据校验结果',
  `if_check_ng_reason` varchar(200) DEFAULT NULL COMMENT '接口数据校验未通过原因',
  `if_create_time` datetime NOT NULL COMMENT '接口数据创建时间',
  PRIMARY KEY (`i_jtg_ep_id`),
  UNIQUE KEY `i_jtg_ep_id` (`i_jtg_ep_id`)
) ENGINE=InnoDB AUTO_INCREMENT=13545351 DEFAULT CHARSET=utf8mb4